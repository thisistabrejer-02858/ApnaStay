<% layout('layouts/boilerplate') %>

<h2 class="mt-4">My Wishlist ❤️</h2>

<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 mt-3">
  <% for(let listing of wishlist) { %>
    <div class="col">
      <div class="card">
        <a href="/listings/<%= listing._id %>">
          <img src="<%= listing.images.url %>" class="card-img-top" style="height:20rem; object-fit:cover;">
        </a>
        <div class="card-body">
          <h5><%= listing.title %></h5>
          ₹<%= listing.price %>/night
          <p><%= listing.country %></p>
        </div>
      </div>
    </div>
  <% } %>
</div>